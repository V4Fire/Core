{
  "name": "V4Fire",
  "tsconfig": "./typedoc.tsconfig.json",
  "entryPoints": ["./src", "./index.d.ts"],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "readme": "./README.md",
  "exclude": ["**/*.spec.js"],
  "hideGenerator": true,
  "gitRevision": "v4",
	"theme": "hierarchy",
	"plugin": ["typedoc-theme-hierarchy"],
  "includes": "./src",
  "media": "./assets",
  "excludePrivate": true,
  "excludeExternals": true
}
